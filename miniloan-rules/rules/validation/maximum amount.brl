  <name>maximum amount</name>
  <uuid>71b49cb5-5392-4057-9466-5c7923dae6a1</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if 
	the amount of 'the loan' is more than 10,000,000 
then
	add "Loan amount exceeds maximum" to the messages of 'the loan' ;
	reject 'the loan' ;
  ]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
